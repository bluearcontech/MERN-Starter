{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },  
  "rules": {
    "semi": [
      2, "never"
    ],
    "linebreak-style": 0,
    "no-console": [
      2,
      {
        "allow": ["log"]
      }
    ],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [
      2,
      {
        "forbid": [
          "any",
          "array"
        ]
      }
    ],
    "react/jsx-first-prop-new-line": [
      2,
      "multiline"
    ],
    "eol-last": ["error", "always"]
  }
}